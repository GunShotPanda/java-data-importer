NULL(http_load_time_delay_count, 0)) != 0 THEN SUM(ISNULL(http_load_time_delay_sum, 0)) / SUM(ISNULL(http_load_time_delay_count, 0)) ELSE 0 END AS 'HTTPLOAD_DELAY', SUM(ISNULL(http_session_count, 0)) AS 'HTTP_SESSION_SUC_COUNT' , SUM(ISNULL(http_request_count, 0)) AS 'HTTPREQCOUNT' , CASE WHEN SUM(ISNULL(http_request_count, 0)) != 0 THEN SUM(ISNULL(http_reply_redirect_count, 0)) / SUM(ISNULL(http_request_count, 0)) * 100 ELSE 0 END AS 'REDIRECT_RATE' , CASE WHEN SUM(ISNULL(http_request_count, 0)) != 0 THEN SUM(ISNULL(http_response_success_count, 0)) / SUM(ISNULL(http_request_count, 0)) * 100 ELSE 0 END AS 'NORMAL_RESPONSE_SUC_RATE' , CASE WHEN SUM(ISNULL(http_get_request_count, 0)) != 0 THEN SUM(ISNULL(http_get_success_count, 0)) / SUM(ISNULL(http_get_request_count, 0)) * 100 ELSE 0 END AS 'HTTPGET_RATE' , CASE WHEN SUM(ISNULL(http_post_request_count, 0)) != 0 THEN SUM(ISNULL(http_post_success_count, 0)) / SUM(ISNULL(http_post_request_count, 0)) * 100 ELSE 0 END AS 'HTTPPOST_RATE' FROM gemstack.wb_http_kpi_15m_uid_app_direct WHERE slicetime >= ? AND ? > slicetime GROUP BY 1, 2, 3, 4, 5) app, (SELECT user_account, cell FROM gemstack.gemstack_pppoe_users) users WHERE app.user_account = users.user_account GROUP BY 1, 2, 3, 4, 5 ) http, (SELECT app.r_stattime, SUM(ISNULL(app.traffic_up,0)) AS 'traffic_up' , SUM(ISNULL(app.traffic_dn, 0)) AS 'traffic_dn' , SUM(ISNULL(app.tcpdatapackage_up, 0)) AS 'tcpdatapackage_up' , SUM(ISNULL(app.tcpdatapackage_dn, 0)) AS 'tcpdatapackage_dn' , SUM(ISNULL(app.tcpconnect_count, 0)) AS 'tcpconnect_count' , SUM(ISNULL(app.tcpsuccessconnect_count, 0)) AS 'tcpsuccessconnect_count' , SUM(ISNULL(app.tcpdatapackage_count, 0)) AS 'tcpdatapackage_count' , SUM(ISNULL(app.tcpretransmit_count, 0)) AS 'tcpretransmit_count' , AVG(ISNULL(app.shakehands_delay, 0)) AS 'shakehands_delay' , AVG(ISNULL(app.client_delay, 0)) AS 'client_delay' , AVG(ISNULL(app.server_delay, 0)) AS 'server_delay' , app.nodetime AS 'nodetime', app.endtime AS 'endtime', users.c